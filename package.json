{
  "name": "lanette",
  "version": "0.0.1",
  "description": "A bot for Pokemon Showdown",
  "main": "app.js",
  "scripts": {
    "mocha": "mocha -R dot -u bdd --delay --no-config test/setup.js",
    "offlinetest": "npm run tslint && npm run offlinemocha",
    "offlinemocha": "mocha -R dot -u bdd --delay --no-config test/setup.js --offline",
    "omocha": "npm run offlinemocha",
    "otest": "npm run offlinetest",
    "test": "npm run tslint && npm run mocha",
    "tsc": "tsc",
    "tslint": "tslint -c tslint.json 'src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sirDonovan/Lanette.git"
  },
  "author": "Quinton Lee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sirDonovan/Lanette/issues"
  },
  "homepage": "https://github.com/sirDonovan/Lanette#readme",
  "dependencies": {
    "@types/mocha": "5.2.7",
    "@types/node": "12.12.26",
    "@types/websocket": "1.0.0",
    "typescript": "3.8.1-rc",
    "websocket": "1.0.31"
  },
  "devDependencies": {
    "mocha": "7.0.0",
    "tslint": "6.0.0",
    "typescript-tslint-plugin": "0.5.5"
  }
}
